{
    "name": "genericwebflexproject",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "cd client && npm run build",
        "install": "cd client && npm install",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "prod": "NODE_ENV=production node server.js",
        "old start": "node ./bin/www",
        "backend prod": "NODE_ENV=production nodemon server.js",
        "backend debug": "ndb server.js",
        "backend watch:js": "parcel watch ./public/javascripts/index.js --out-dir ./public/javascripts --out-file bundle.js",
        "backend build:js": "parcel build ./public/javascripts/index.js --out-dir ./public/javascripts --out-file bundle.js"
    },
    "dependencies": {
        "compression": "^1.7.4",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "^2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "express-mongo-sanitize": "^2.0.0",
        "express-rate-limit": "^5.2.3",
        "helmet": "^4.2.0",
        "hpp": "^0.2.3",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mongodb": "^3.6.3",
        "mongoose": "^5.10.15",
        "morgan": "^1.9.1",
        "nodemon": "^2.0.6",
        "pug": "2.0.0-beta11",
        "slugify": "^1.4.6",
        "validator": "^13.1.17",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "eslint": "^7.13.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.0.0",
        "ndb": "^1.1.5",
        "prettier": "^2.2.0"
    }
}
